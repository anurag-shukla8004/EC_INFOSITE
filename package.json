{
  "name": "startd-theme",
  "description": "A TypeScript/Next.js theme that includes everything you need to build amazing landing page!",
  "version": "1.0.0",
  "private": true,
  "author": "Jaakko Kytölä <kytola.jaakko@gmail.com> (jaakkokytola.com)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript",
    "tailwind",
    "theme"
  ],
  "scripts": {
    "dev": "next -p 8005",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@fontsource/inter": "4.2.1",
    "@fontsource/jost": "4.5.10",
    "@netlify/ipx": "^1.2.1",
    "@netlify/plugin-nextjs": "^3.6.2",
    "@sendgrid/mail": "^7.7.0",
    "@svgr/webpack": "5.5.0",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.29.1",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "ipx": "^0.9.10",
    "netlify-plugin-ipx": "^0.0.1",
    "next": "^12.2.5",
    "next-seo": "4.20.0",
    "nextjs-cors": "^2.1.1",
    "npm": "^8.17.0",
    "postcss": "^8.4.16",
    "radium": "0.26.2",
    "react": "17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.4.0",
    "react-youtube": "9.0.3",
    "run": "^1.4.0",
    "swr": "^1.3.0",
    "tailwindcss": "^3.1.8",
    "twind": "0.15.4",
    "uglify-js": "^3.16.3",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-dom": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "commitizen": "4.2.3",
    "cypress": "^10.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.0.9",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
