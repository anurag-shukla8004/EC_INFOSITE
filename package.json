{
  "name": "startd-theme",
  "description": "A TypeScript/Next.js theme that includes everything you need to build amazing landing page!",
  "version": "1.0.0",
  "private": true,
  "author": "Jaakko Kytölä <kytola.jaakko@gmail.com> (jaakkokytola.com)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript",
    "tailwind",
    "theme"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:prod": "next start -p 30",
    "export":"next build && next export",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@svgr/webpack": "5.5.0",
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "next": "^12.2.5",
    "next-seo": "4.20.0",
    "react": "17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.4.0",
    "react-phone-number-input": "^3.2.11",
    "react-youtube": "9.0.3",
    "twind": "0.15.4",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.18",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.20.0",
    "husky": "5.0.9",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "^4.8.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
